<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayoutBF"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Nurse.BreastFeedingActivity">

    <ProgressBar
        android:id="@+id/progressBarBF"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_centerInParent="true"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:visibility="gone" />

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.appcompat.widget.LinearLayoutCompat
            android:id="@+id/layoutBF"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:padding="10dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:background="@drawable/customborder">

                <TextView
                    android:id="@+id/textView126"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginBottom="10dp"
                    android:maxWidth="250dp"
                    android:text="@string/secN_breast_advantageBreastMilk"
                    android:textSize="18sp" />

                <CheckBox
                    android:id="@+id/checkBoxAdvantage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"/>

                <TextView
                    android:id="@+id/txtViewAdvantageDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textView126"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:text="@string/secN_Date"
                    android:textColor="@android:color/holo_green_dark" />

                <TextView
                    android:id="@+id/txtViewAdvantagePerson"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_below="@id/textView126"
                    android:layout_toRightOf="@id/txtViewAdvantageDate"
                    android:text="@string/secN_medicalPersonnale_name"
                    android:textColor="@color/colorAccent" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:background="@drawable/customborder">

                <TextView
                    android:id="@+id/textView129"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginBottom="10dp"
                    android:maxWidth="250dp"
                    android:text="@string/secN_breast_exclusive_feeding"
                    android:textSize="18sp" />

                <CheckBox
                    android:id="@+id/checkBoxBFExclusive"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"/>

                <TextView
                    android:id="@+id/txtViewBFExclusiveDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textView129"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:text="@string/secN_Date"
                    android:textColor="@android:color/holo_green_dark" />

                <TextView
                    android:id="@+id/txtViewBFExclusivePerson"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_below="@id/textView129"
                    android:layout_toRightOf="@id/txtViewBFExclusiveDate"
                    android:text="@string/secN_medicalPersonnale_name"
                    android:textColor="@color/colorAccent" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:background="@drawable/customborder">

                <TextView
                    android:id="@+id/textView132"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginBottom="10dp"
                    android:maxWidth="250dp"
                    android:text="@string/secN_breast_early_breastfeeding"
                    android:textSize="18sp" />

                <CheckBox
                    android:id="@+id/checkBoxEarlyBF"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"/>

                <TextView
                    android:id="@+id/txtViewEarlyBFDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textView132"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:text="@string/secN_Date"
                    android:textColor="@android:color/holo_green_dark" />

                <TextView
                    android:id="@+id/txtViewEarlyBFPerson"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_below="@id/textView132"
                    android:layout_toRightOf="@id/txtViewEarlyBFDate"
                    android:text="@string/secN_medicalPersonnale_name"
                    android:textColor="@color/colorAccent" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:background="@drawable/customborder">

                <TextView
                    android:id="@+id/textView135"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginBottom="10dp"
                    android:maxWidth="250dp"
                    android:text="@string/secN_breast_rooming_in"
                    android:textSize="18sp" />

                <CheckBox
                    android:id="@+id/checkBoxRoomingIn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"/>

                <TextView
                    android:id="@+id/txtViewRoomingInDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textView135"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:text="@string/secN_Date"
                    android:textColor="@android:color/holo_green_dark" />

                <TextView
                    android:id="@+id/txtViewRoomingInPerson"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_below="@id/textView135"
                    android:layout_toRightOf="@id/txtViewRoomingInDate"
                    android:text="@string/secN_medicalPersonnale_name"
                    android:textColor="@color/colorAccent" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:background="@drawable/customborder">

                <TextView
                    android:id="@+id/textView138"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginBottom="10dp"
                    android:maxWidth="250dp"
                    android:text="@string/secN_breast_brestFedding_methods"
                    android:textSize="18sp" />

                <CheckBox
                    android:id="@+id/checkBoxBFMethod"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"/>

                <TextView
                    android:id="@+id/txtViewBFMethodDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textView138"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:text="@string/secN_Date"
                    android:textColor="@android:color/holo_green_dark" />

                <TextView
                    android:id="@+id/txtViewBFMethodPerson"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_below="@id/textView138"
                    android:layout_toRightOf="@id/txtViewBFMethodDate"
                    android:text="@string/secN_medicalPersonnale_name"
                    android:textColor="@color/colorAccent" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:background="@drawable/customborder">

                <TextView
                    android:id="@+id/textView141"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginBottom="10dp"
                    android:maxWidth="250dp"
                    android:text="@string/secN_breast_baby_needs"
                    android:textSize="18sp" />

                <CheckBox
                    android:id="@+id/checkBoxBFBabyNeeds"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"/>

                <TextView
                    android:id="@+id/txtViewBFBabyNeedsDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textView141"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:text="@string/secN_Date"
                    android:textColor="@android:color/holo_green_dark" />

                <TextView
                    android:id="@+id/txtViewBFBabyNeedsPerson"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_below="@id/textView141"
                    android:layout_toRightOf="@id/txtViewBFBabyNeedsDate"
                    android:text="@string/secN_medicalPersonnale_name"
                    android:textColor="@color/colorAccent" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:background="@drawable/customborder">

                <TextView
                    android:id="@+id/textView144"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginBottom="10dp"
                    android:maxWidth="250dp"
                    android:text="@string/secN_breast_determine_milk"
                    android:textSize="18sp" />

                <CheckBox
                    android:id="@+id/checkBoxBreastMilk"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"/>

                <TextView
                    android:id="@+id/txtViewBreastMilkDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textView144"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:text="@string/secN_Date"
                    android:textColor="@android:color/holo_green_dark" />

                <TextView
                    android:id="@+id/txtViewBreastMilkPerson"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_below="@id/textView144"
                    android:layout_toRightOf="@id/txtViewBreastMilkDate"
                    android:text="@string/secN_medicalPersonnale_name"
                    android:textColor="@color/colorAccent" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:background="@drawable/customborder">

                <TextView
                    android:id="@+id/textView147"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginBottom="10dp"
                    android:maxWidth="250dp"
                    android:text="@string/secN_breast_solve_breastfeeding_problem"
                    android:textSize="18sp" />

                <CheckBox
                    android:id="@+id/checkBoxBFProblem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"/>

                <TextView
                    android:id="@+id/txtViewBFProblemDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textView147"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:text="@string/secN_Date"
                    android:textColor="@android:color/holo_green_dark" />

                <TextView
                    android:id="@+id/txtViewBFProblemPerson"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_below="@id/textView147"
                    android:layout_toRightOf="@id/txtViewBFProblemDate"
                    android:text="@string/secN_medicalPersonnale_name"
                    android:textColor="@color/colorAccent" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:background="@drawable/customborder">

                <TextView
                    android:id="@+id/textView150"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginBottom="10dp"
                    android:maxWidth="250dp"
                    android:text="@string/secN_breast_breastfeeding_work_mummy"
                    android:textSize="18sp" />

                <CheckBox
                    android:id="@+id/checkBoxBFWorkMother"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"/>

                <TextView
                    android:id="@+id/txtViewBFWorkMotherDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textView150"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:text="@string/secN_Date"
                    android:textColor="@android:color/holo_green_dark" />

                <TextView
                    android:id="@+id/txtViewBFWorkMotherPerson"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_below="@id/textView150"
                    android:layout_toRightOf="@id/txtViewBFWorkMotherDate"
                    android:text="@string/secN_medicalPersonnale_name"
                    android:textColor="@color/colorAccent" />
            </RelativeLayout>

            <com.google.android.material.button.MaterialButton
                android:id="@+id/btnBFSave"
                style="@style/Button"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:text="@string/save"
                app:cornerRadius="16dp" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/btnBFCancel"
                style="@style/cancelButton"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_gravity="bottom"
                android:text="@string/cancel"
                app:cornerRadius="16dp" />

        </androidx.appcompat.widget.LinearLayoutCompat>

    </androidx.core.widget.NestedScrollView>

</RelativeLayout>