<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/selfCheckFragment">

    <fragment
        android:id="@+id/selfCheckFragment"
        android:name="com.example.mommyhealthapp.Mommy.ui.SelfCheck.SelfCheckFragment"
        android:label="@string/self_check"
        tools:layout="@layout/fragment_self_check" >
        <action
            android:id="@+id/action_selfCheckFragment_to_navigation_mother_profile2"
            app:destination="@id/navigation_mother_profile" />
        <action
            android:id="@+id/action_selfCheckFragment_to_navigation_pregnancy_record2"
            app:destination="@id/navigation_pregnancy_record" />
        <action
            android:id="@+id/action_selfCheckFragment_to_navigation_notifications2"
            app:destination="@id/navigation_notifications" />
        <action
            android:id="@+id/action_selfCheckFragment_self"
            app:destination="@id/selfCheckFragment" />
    </fragment>

    <fragment
        android:id="@+id/navigation_mother_profile"
        android:name="com.example.mommyhealthapp.Mommy.ui.MotherProfile.MotherProfileFragment"
        android:label="@string/mother_profile"
        tools:layout="@layout/fragment_mother_profile" />

    <fragment
        android:id="@+id/navigation_pregnancy_record"
        android:name="com.example.mommyhealthapp.Mommy.ui.PregnancyRecord.PregnancyRecordFragment"
        android:label="@string/pregnancy_record"
        tools:layout="@layout/fragment_pregnancy_record" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.mommyhealthapp.Mommy.ui.Notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />

</navigation>